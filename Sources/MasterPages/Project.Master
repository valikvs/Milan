<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Project.Master.cs"
    Inherits="VSS.Milan.Web.MasterPages.Project" MasterPageFile="~/masterpages/MainContent.master" %>
<%@ Import Namespace="VSS.Milan.Web.Core.Constants" %>
<%@ Import Namespace="VSS.Milan.Web.Core.Extentions" %>
<%@ Import Namespace="VSS.Milan.Web.Core.Utils" %>
<%@ Register TagPrefix="umbraco" Namespace="umbraco.presentation.templateControls"
    Assembly="umbraco" %>
<%@ Register TagPrefix="uc" TagName="Sharing" Src="~/UserControls/Sharing.ascx" %>
<asp:Content ContentPlaceHolderID="ContentPlaceHolderMain" runat="server">
  <div class="mainCol clearfix galleryCol">
    <h3><%=CurrentNode.Property(Fields.Project.Title)%></h3>
    <%if (this.NextProject != null){%>
    <a href="<%=this.NextProject.Url%>" class="nextProject" title="<%=this.ProjectsOverview.Property(Fields.ProjectsOverview.ViewNextProject) %>"></a>
    <%}%>
    <%if (this.PreviousProject != null){%>
    <a href="<%=this.PreviousProject.Url%>" class="prevProject" title="<%=this.ProjectsOverview.Property(Fields.ProjectsOverview.ViewPreviousProject) %>"></a>
    <%}%>
    <%if (this.Images.Count > 0){%>
    <div id="gallery" class="ad-gallery">
      <div class="ad-image-wrapper"> </div>
      <div class="ad-nav">
        <div class="ad-thumbs">
          <ul class="ad-thumb-list">
            <%foreach (var image in this.Images) {%>
            <%=HtmlHelper.ProjectImage(image)%>
            <%}%>
          </ul>
        </div>
      </div>
    </div>
    <%}%>
    <uc:Sharing runat="server"/>
  </div>
    <div class="sideCol">
        <umbraco:Macro ID="uProjectsNavigation" Alias="ProjectsNavigation" runat="server" />
    </div>
    <div class="clear he">
        <!---->
    </div>
</asp:Content>
